          <Image class="optimization centerArrow" src={centerArrow} alt="" />
        </a>
      </div>
    </div>
    <Image class="optimization " style="width:100%;" src={awardsNew} alt="" />
  </section>

  <section style="padding: 5% 2%;">
    <div class="swiper">
      <div class="swiper-wrapper">
        {
          awardsData.items.map((awards, index) => (
            <div class="swiper-slide">
              <div class="row desktopSlider">
                <div class="col-7 leftSide">
                  <div class="award-name">
                    <span set:html={awards.fields.awardName} />
                  </div>
                  <div class="award-category">
                    <span
                      class="category-name"
                      set:html={awards.fields.category}
                    />
                    <span class="year" set:html={awards.fields.rankAndYear} />
                  </div>

                  <div class="project">
                    <div class="row">
                      <div class="col-1 projectNum">
                        <span class="index">{(index = index + 1)}</span>
                        <span class="length">/39</span>
                      </div>
                      <div class="col-lg-6 col-md-6 col-sm-5 projectName">
                        {awards.fields.projectName &&
                        <p>PROJECT:</p>
                        <span set:html={awards.fields.projectName} />
                        }
                      </div>
                      <div class="col-lg-3 col-md-3 col-sm-3">
                        { awards.fields.projectReference?.fields?.heroTitle &&
                        
                        <a
                          href={
                            "/portfolio/" +
                            awards.fields.projectReference?.fields?.heroTitle
                          }
                        >
                          <Image
                            class="viewProject optimization"
                            src={viewProjectBtn}
                            alt=""
                            loading="lazy"
                          />
                        </a>
                      }
                      </div>
                    </div>
                  </div>
                </div>
                <div class="col-4 awardImgDiv ">
                  <div class="awardImg">
                   
                      <Image
                        class="momento resize optimization"
                        src={awards?.fields?.awardImage?.fields?.file.url}
                        alt=""
                        loading="lazy"
                        width={444}
                        height={610}
                      />
                      <br />
                  {awards?.fields?.awardBrandLogo && 
                  
                    <Image
                      class="momentoIcon optimization"
                      src={awards?.fields?.awardBrandLogo && awards?.fields?.awardBrandLogo?.fields?.file.url}
                      alt=""
                      loading="lazy"
                      width={330}
                        height={130}
                    />
                  }
                  </div>
                </div>
              </div>

              <div class="onlyMob">
                <div class="mb-award-name">
                  <span set:html={awards.fields.awardName} />
                </div>
                <div class="d-flex">
                  <div style="width: 45%;">
                    <div class="mb-award-category">
                      <span
                        class="category-name"
                        set:html={awards.fields.category}
                      />
                      <span class="year" set:html={awards.fields.rankAndYear} />
