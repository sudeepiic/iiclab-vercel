      .from(".viewAwardsBtn, .centerArrow", {
        opacity: 0,
      })

      .from(".viewAwardsBtn", {
        rotate: "360",
        duration: 10,
        repeat: -1,
        ease: "none",
      });
   
    const items = document.querySelectorAll(".trial");

    items.forEach((el) => {
      const image = el.querySelector(".trial2");

      el.addEventListener("mouseenter", (e) => {
        gsap.to(image, { autoAlpha: 1 });
      });

      el.addEventListener("mouseleave", (e) => {
        gsap.to(image, { autoAlpha: 0 });
      });

      el.addEventListener("mousemove", (e: PointerEvent) => {
        gsap.to(image, { x: e.offsetX - 300 });
      });
    });

