  <script>
    document.querySelector(".imgDiv").classList.add("imgDiv-animate");

    import { gsap } from "gsap";
    import { ScrollTrigger } from "gsap/ScrollTrigger";
    // import Eases from "gsap";
    gsap.registerPlugin(ScrollTrigger);

    let tl = gsap.timeline();

    tl.from(".title1", {
      duration: 1,
      x: "-100%",
      opacity: 0.5,
      ease: "power1.out",
    })
      .from(".title2", {
        duration: 1,
