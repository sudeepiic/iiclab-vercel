---
import logo from "../images/logo.png";
import blackCircle from "../images/blackcircle.png";
import whiteCircle from "../images/whitecircle.png";
import calllogo from "../images/calllogo.png";
import logoSvg from "../images/logoSvg.svg"
---

<header class="turnBlack">
  <div class="d margins ">
    <nav class="main d-inline-flex">
      <div class="logo">
        <a href="/">
          <!-- <img  src={logoSvg} alt="" /> -->
          <svg width="44" height="48" viewBox="0 0 44 48" fill="none" xmlns="http://www.w3.org/2000/svg">
            <path fill-rule="evenodd" clip-rule="evenodd" d="M27.8125 47.4504C27.8125 31.7498 27.8125 16.0492 27.8125 0.348633C33.1525 0.348633 38.4469 0.348633 43.8326 0.348633C43.8326 4.04558 43.8326 7.74252 43.8326 11.4851C42.2351 11.4851 40.6833 11.4851 39.0403 11.4851C39.0403 19.7462 39.0403 27.9616 39.0403 36.2683C40.6377 36.2683 42.1895 36.2683 43.8326 36.2683C43.8326 40.0109 43.8326 43.7078 43.8326 47.4504C38.4926 47.4504 33.1982 47.4504 27.8125 47.4504Z" fill="black"/>
            <path fill-rule="evenodd" clip-rule="evenodd" d="M13.8828 47.4504C13.8828 31.7498 13.8828 16.0492 13.8828 0.348633C17.5798 0.348633 21.3223 0.348633 25.0649 0.348633C25.0649 16.0492 25.0649 31.7498 25.0649 47.4504C21.3223 47.4504 17.6254 47.4504 13.8828 47.4504Z" fill="black"/>
            <path fill-rule="evenodd" clip-rule="evenodd" d="M11.1365 47.4508C7.43953 47.4508 3.74258 47.4508 0 47.4508C0 37.5466 0 27.6424 0 17.7383C3.69694 17.7383 7.39389 17.7383 11.1365 17.7383C11.1365 27.6424 11.1365 37.5466 11.1365 47.4508Z" fill="black"/>
            </svg>

        </a>
      </div>
      <div class="list onlyDesk">
        <ul class="d-inline-flex">
          <li class="paddingsLi"><a class="navTabs" href="">Our Work</a></li>
          <li><a class="navTabs" href="">Solutions</a></li>
          <li><a class="navTabs" href="/about-us/">Life at IIC</a></li>
          <li><a class="navTabs" href="">Career</a></li>
          <li><a class="navTabs" href="/blog-gallery/">Articles</a></li>
        </ul>
      </div>
      <div class="contact-div">
        <div class="div1">
          <div class="div2"><img src={blackCircle} alt="" /></div>
          <div class="div3"><img src={whiteCircle} alt="" /></div>
          <div class="div4"><img src={calllogo} alt="" /></div>
          <p>Connect</p>
        </div>
      </div>
      <div class="menu d-flex">
        <p>menu</p>
        <span class="s1"></span>
        <span class="s2"></span>
      </div>
    </nav>
  </div>
</header>

<style>
  .margins {
    margin-left: 7%;
    margin-right: 7%;
  }
  :root {
    --mainfont-font-family: "Anek Devanagari";
    --maincolor-color: #1a1a1a;
    
  }

  .main {
    height: 60px;
    width: 100%;
    margin-top: 30px;
    /* border:1px solid black; */
  }
  .main .logo {
    margin-top: 10px;
  }
  .main img {
    height: 40px;
  }
  .main ul {
    list-style: none;
  }
  .list {
  }
  .list ul li {
    padding: 17px;
  }
  .list ul li a {
    text-decoration: none;
    font-family: var(--mainfont-font-family);
    font-style: normal;
    font-weight: 400;
    font-size: 16px;
    text-transform: capitalize;
    color: #061b28;
  }
  .navTabs {
    opacity: 50%;
  }


 
@media screen and (max-width:992px){
  .list ul li{
    padding-left:8px !important;
    padding-right:8px !important;
  }
}



  .contact-div {
    position: relative;
    /* border: 1px solid black; */
    margin-left: auto;
  }
  .contact-div .div1 {
    position: relative;
    border: 1px solid black;
    background-color: var(--maincolor-color);
    height: 52px;
    width: 151px;
    border-radius: 31px;
    margin-right: 30px;
  }
  .contact-div .div2 {
    position: absolute;
    margin-top: 11px;
    margin-left: 25px;
  }
  .contact-div .div2 img {
    height: 30px;
    width: 30px;
  }
  .contact-div .div3 {
    position: absolute;
    margin-left: 29.5px;
    margin-top: 12px;
  }
  .contact-div .div3 img {
    height: 22px;
    width: 22px;
  }
  .contact-div .div4 {
    position: absolute;
    margin-top: 12px;
    margin-left: 36px;
  }
  .contact-div .div4 img {
    width: 10px;
    height: 10px;
  }
  .contact-div p {
    color: #ffffff;
    margin-left: 65px;
    margin-top: 14px;
    font-family: var(--mainfont-font-family);
    font-style: normal;
  }

  .menu {
    /* margin-left: 180px; */
  }
  .menu p {
    margin-top: 15px;
    font-weight: 400;
    font-family: var(--mainfont-font-family);
    text-transform: capitalize;
  }
  .menu .s1 {
    border: 1px solid black;
    transform: rotate(90deg);
    height: 25px;
    margin-left: 20px;
    margin-top: 10px;
  }
  .menu .s2 {
    border: 1px solid black;
    transform: rotate(90deg);
    height: 40px;
    margin-top: 10px;
    margin-left: 6px;
  }
  .black{
    background-color: #1a1a1a;
    color: white;
  }
  @media screen and (max-width:768px){
    .onlyDesk{
      display: none;
    }
    .contact-div{
      display: none;
    }
    .menu{
     position: absolute;
     right: 7%;
    }
  }

 
</style>
