const entries: any = await contentfulClient.getEntries<any>({
  content_type: "caseStudy",
});
const clientsEntries = await contentfulClient.getEntries<any>({
  content_type: "clients",
});
const images = clientsEntries.items[0].fields.clientIcons;
// const clientLogoes = clientsEntries.items[0].fields.clientLogoes;

const imagesPerArray = 5;
const numArrays = Math.ceil((images as []).length / imagesPerArray);

const splitArrays = [];
for (let i = 0; i < numArrays; i++) {
  const startIndex = i * imagesPerArray;
  const endIndex = startIndex + imagesPerArray;
  const splitArray = (images as []).slice(startIndex, endIndex);
  splitArrays.push(splitArray);
}

const testimonials: any = await contentfulClient.getEntries<any>({
  content_type: "clientsTestimonials",
});
const bgVideoReel: any = await contentfulClient.getEntries<any>({
  content_type: "showreel",
});
const showreelVideo: any = await contentfulClient.getEntries<any>({
  content_type: "showreel2",
});
const bgVideo = bgVideoReel.items[0].fields.video.fields.file.url;
const showreel = showreelVideo.items[0].fields.video.fields.file.url;
